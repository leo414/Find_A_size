{
  "name": "Find_A_size",
  "version": "0.0.1",
  "description": "find a size from Amazon",
  "main": "app/index.jsx",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --progress --host 0.0.0.0",
    "build": "./node_modules/.bin/webpack --progress --colors --config webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint app",
    "clean": "rm -rf build/*",
    "test": "./node_modules/.bin/mocha --recursive --compilers js:babel-core/register --require ./test/setup.js",
    "test:watch": "npm run test -- --watch"
  },
  "betterScripts": {
    "dev": {
      "command": "./node_modules/.bin/webpack-dev-server --progress",
      "env": {
        "BUILD_DEV": "development"
      }
    },
    "build": {
      "env": {
        "BUILD_PRERELEASE": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "keywords": [
    "react",
    "webpack",
    "es6",
    "babel"
  ],
  "author": "zaxlct",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leo414/Find_A_size/issues"
  },
  "homepage": "https://github.com/leo414/Find_A_size#readme",
  "dependencies": {
    "antd": "^2.0.1",
    "better-npm-run": "0.0.8",
    "classnames": "^2.2.5",
    "jquery": "^3.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-mixin": "^3.0.5",
    "react-router": "^2.8.1",
    "react-router-transition": "0.0.6",
    "reflux": "^0.4.1"
  },
